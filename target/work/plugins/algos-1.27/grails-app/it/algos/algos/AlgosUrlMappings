class UrlMappings {

	static mappings = {
		"/$controller/$action?/$id?"{
			constraints {
				// apply constraints here
			}
		}

		"/"(view:"/index")
        "/home"(view:"/index")
        "/gac"(view:"/gac")
		"500"(view:'/error')
    } // end of static mapping

}// fine della classe UrlMappings
