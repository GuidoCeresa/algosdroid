<plugin name='algosvers' version='0.30' grailsVersion='2.2.3 &gt; *'>
  <author>Gac</author>
  <title>Algosvers Plugin</title>
  <description>Brief summary/description of the plugin.
</description>
  <documentation>http://grails.org/plugin/algosvers</documentation>
  <type>AlgosversGrailsPlugin</type>
  <resources>
    <resource>VersioneBootStrap</resource>
    <resource>spring.resources</resource>
    <resource>it.algos.algosvers.VersioneController</resource>
    <resource>it.algos.algosvers.Versione</resource>
    <resource>it.algos.algosvers.VersioneService</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
